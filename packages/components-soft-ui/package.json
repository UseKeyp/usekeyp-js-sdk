{
  "name": "@usekeyp/components-soft-ui",
  "version": "0.1.1",
  "type": "module",
  "private": false,
  "main": "src/components/index.js",
  "dependencies": {
    "@redwoodjs/router": "^3.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@usekeyp/tailwind-presets": "^0.1.1",
    "chart.js": "^4.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-table": "^7.8.0",
    "react-tooltip": "^4.5.0",
    "surge": "^0.23.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "start": "styleguidist server --config styleguide.config.cjs",
    "tailwindcss": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "build": "styleguidist build --config styleguide.config.cjs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pre-publish": "exit 0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-no-undef": [
        "error",
        {
          "allowGlobals": true
        }
      ],
      "react/no-unescaped-entities": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "eslint": "^8.27.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.4.18",
    "process": "^0.11.10",
    "react-styleguidist": "^13.0.0",
    "tailwindcss": "^3.2.1",
    "url-loader": "^4.1.1"
  }
}
