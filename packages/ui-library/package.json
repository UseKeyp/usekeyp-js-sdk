{
  "name": "@usekeyp/ui-library",
  "version": "0.1.2",
  "description": "Component library for building apps using Keyp",
  "author": "alekseevayana <alekseevayana@bk.ru>",
  "homepage": "https://github.com/UseKeyp/ui-library#readme",
  "license": "ISC",
  "main": "lib/components/index.js",
  "files": [
    "plugin.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UseKeyp/ui-library.git"
  },
  "scripts": {
    "start": "styleguidist server --config styleguide.config.cjs",
    "tailwindcss": "tailwindcss -i ./src/index.css -o ./dist/output.css --watch -c tailwind.config.js",
    "build": "yarn tailwindcss && styleguidist build --config styleguide.config.cjs && babel --config-file ./babel.config.js src -d lib",
    "pre-publish": "babel --config-file ./babel.config.js src -d lib ",
    "test": "node ./__tests__/ui-library.test.js"
  },
  "dependencies": {
    "@redwoodjs/router": "^3.3.0",
    "prop-types": "^15.8.1",
    "react-styleguidist": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.3.1"
  }
}
